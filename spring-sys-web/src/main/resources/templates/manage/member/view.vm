                    #formInput({"name": "保值积分"} {"name": "accountJsyPoint", "value":"$!{member.accountJsyPoint}","id": "accountJsyPoint","required": "required","readonly":"readonly"})
                    #formInput({"name": "金蛋"} {"name": "score", "value":"$!{member.score}","id": "score","required": "required","readonly":"readonly"})

                    #formInput({"name": "身份证号码"} {"name": "cartId", "value":"$!{member.cartId}","id": "cartId","required": "required","readonly":"readonly"})
                    #formInput({"name": "银行卡号"} {"name": "bankCardId", "value":"$!{member.bankCardId}","id": "bankCardId","required": "required","readonly":"readonly"})
