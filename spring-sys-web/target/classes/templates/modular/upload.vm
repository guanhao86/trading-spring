<div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="upload_modal" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                <h4 class="modal-title">导入EXCEL文件</h4>
            </div>
            <div class="modal-body">
                <form id="form_upload" class="cmxform form-horizontal adminex-form"  method="post" enctype="multipart/form-data" action="" >
